       \printtext[parens]{%
         \printfield{eventtitle}%
         \setunit{\addcolondelim}%
         \printfield{eventtitleaddon}%
         \setunit{\addcomma\space}%
         \printfield{venue}%
         \setunit*{\addcomma\space}%
         \printeventdate}}%
  \newunit}

